{
  "@context": "http://schema.org",
  "@type": "WebSite",
  "url": "http://eventbrite.com/",
  "potentialAction": {
    "@type": "SearchAction",
    "actionStatus": "PotentialActionStatus",
    "target": "http://eventbrite.com/search",
    "result-output": "required",
    "object": {
      "@type": "Event",
      "name-input": {
        "@type": "PropertyValueSpecification",
        "valueName": "Event Name"
      },
      "startDate-input": {
        "@type": "PropertyValueSpecification",
        "valueName": "Start Date"
      },
      "endDate-input": {
        "@type": "PropertyValueSpecification",
        "valueName": "End Date"
      },
      "location": {
        "@type": "Place",
        "address": {
          "@type": "PostalAddress",
          "addressLocality-input": {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "defaultValue": "Innsbruck",
            "valueName": "city"
          },
          "addressCountry-input": {
            "@type": "PropertyValueSpecification",
            "valueRequired": true,
            "defaultValue": "Austria",
            "valueName": "country"
          }
        }
      }
    }
  }
}

{
  "@context": "http://schema.org",
    "@type": "BuyAction",
    "actionStatus": "PotentialActionStatus",
    "target": "http://example.com/search?&q={query}",
    "object": {
      "@type": "Event"
    },
    "result-output": "required",

    "query-input": {
      "@type": "PropertyValueSpecification",
      "valueRequired": true,
      "valueName": "Event Name"
    },
    "location": {
      "@type": "Place",
      "address": {
        "@type": "PostalAddress",
        "addressLocality-input": {
          "@type": "PropertyValueSpecification",
          "valueRequired": true,
          "defaultValue": "Innsbruck",
          "valueName": "city"
        },
        "addressCountry-input": {
          "@type": "PropertyValueSpecification",
          "valueRequired": true,
          "defaultValue": "Austria",
          "valueName": "country"
        }
      }
    }
  }
